{
  "version": 2,
  "builds": [
    {
      "src": "example/manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": ["requirements.txt"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "example/manage.py"
    }
  ]
}
